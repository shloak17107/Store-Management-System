<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.loginController">
   <children>
      <Button fx:id="btnLogin" layoutX="546.0" layoutY="319.0" mnemonicParsing="false" text="Login">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <TextField fx:id="txLogin" disable="true" layoutX="467.0" layoutY="171.0" promptText="LoginID">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txPswd" disable="true" layoutX="467.0" layoutY="234.0" promptText="Password">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblUser" alignment="CENTER" layoutX="480.0" layoutY="101.0" prefHeight="19.0" prefWidth="170.0" text="User" textFill="#bf2727">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="120.0" layoutY="101.0" prefHeight="19.0" prefWidth="157.0" text="Select User">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="Super_User" alignment="CENTER" layoutX="120.0" layoutY="170.0" mnemonicParsing="false" onAction="#Users_button" prefHeight="30.0" prefWidth="150.0" text="Super User" />
      <Button fx:id="Warehouse_Admin" layoutX="120.0" layoutY="230.0" mnemonicParsing="false" onAction="#Users_button" prefHeight="30.0" prefWidth="150.0" text="Warehouse Admin" />
      <Button fx:id="Store_Admin" alignment="CENTER" layoutX="120.0" layoutY="290.0" mnemonicParsing="false" onAction="#Users_button" prefHeight="30.0" prefWidth="150.0" text="Store Admin" />
      <Button fx:id="EndUser" alignment="CENTER" layoutX="120.0" layoutY="350.0" mnemonicParsing="false" onAction="#Users_button" prefHeight="30.0" prefWidth="150.0" text="End User" />
   </children>
</AnchorPane>
